{
  "name": "camp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "./node_modules/.bin/ng serve",
    "build:client-app:prod": "./node_modules/.bin/ng build --prod",
    "build:server-app:prod": "./node_modules/.bin/ng run camp:server",
    "build-all": "./node_modules/.bin/npm-run-all build:client-app:prod build:server-app:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "ng run camp:prerender",
    "prerender:debug": "./node_modules/.bin/ts-node --inspect ./prerender.ts",
    "start:express-server": "./node_modules/.bin/ts-node ./server.ts",
    "dev:ssr": "ng run camp:serve-ssr",
    "serve:ssr": "node dist/camp-server/main.js",
    "build:ssr": "ng build --prod && ng run camp:server:production"
  },
  "private": true,
