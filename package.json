    "@angular/platform-server": "9.1.5",
    "@angular/router": "9.1.5",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@nguniversal/common": "^9.1.0",
    "@nguniversal/express-engine": "^9.1.0",
